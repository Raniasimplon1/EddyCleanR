year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{EddyCleanR}: Functions to filter and gap fill eddy covariance data and calculate et values",
         author = c(person("Adam", "Sparks")),
         year = year,
         note = note,
         url = "https://github.com/adamhsparks/EddyCleanR")
